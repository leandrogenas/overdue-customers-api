  "main": "main.js",
  "scripts": {
    "start": "concurrently --kill-others npm:fe:build npm:be:start ",
    "start:dev": "concurrently --kill-others npm:fe:watch npm:be:start:dev npm:open ",
    "open": "electron .",
    "fe:ng": "ng",
    "fe:watch": "ng build --delete-output-path false --progress --watch",
    "fe:start": "ng serve",
    "fe:build": "ng build --progress",
    "fe:build:prod": "ng build --progress --prod",
    "fe:test": "ng test",
    "fe:lint": "ng lint",
    "fe:e2e": "ng e2e",
    "be:prebuild": "rimraf dist",
    "be:build": "nest build",
    "be:format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "be:start": "nest start",
    "be:start:dev": "nest start --watch",
    "be:start:debug": "nest start --debug --watch",
    "be:start:prod": "node dist/main",
    "be:lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "be:test": "jest",
    "be:test:watch": "jest --watch",
    "be:test:cov": "jest --coverage",
    "be:test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "be:test:e2e": "jest --config ./test/jest-e2e.json",
    "postinstall": "echo show"
  },
